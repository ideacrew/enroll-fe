{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".github/workflows/test-and-deploy.yml": "*",
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "trunk"
  },
  "npmScope": "enroll",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "accessToken": "MzIyYWQxZTctMDNjMS00MGY4LTg0OWUtOThlZTUyMjU2YTVjfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    }
  },
  "cli": {
    "packageManager": "npm"
  },
  "defaultProject": "coverme",
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
